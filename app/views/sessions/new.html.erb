<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |form| %>

  <div class="session_form">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div class="session_form">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <%= form.submit "Log in" %>
<% end %>